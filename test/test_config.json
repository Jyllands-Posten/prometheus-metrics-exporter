{
  "pushGatewayUrl": "http://pushgateway:9091",
  "basicAuth": {
    "username": "username",
    "password": "password"
  },
  "scrapeTargets": [
    {
      "url": "https://webserver/html",
      "metrics": [
        {
          "name": "test",
          "help": "test html help text",
          "path": "json.path.01",
          "instrumentType": "gauge"
        }
      ],
      "labels": [
        {"name": "TestHtmlBasicAuthLabelName", "value": "TestHtmlBasicAuthLabelValue"}
      ],
      "mimeType": "html",
      "jobName": "TestHtmlBasicAuthJobName",
      "timeoutInSecs": 15
    },
    {
    "url": "https://webserver/json",
    "metrics": [
      {
        "name": "test",
        "help": "test json help text",
        "path": "json.path.01",
        "instrumentType": "gauge"
      }
    ],
    "labels": [
      {"name": "TestJsonBasicAuthLabelName", "value": "TestJsonBasicAuthLabelValue"}
    ],
    "mimeType": "html",
    "jobName": "TestJsonBasicAuthJobName",
    "timeoutInSecs": 15
    }
  ]
}
